<h1>Listing car_res</h1>

<table>
  <thead>
    <tr>
      <th>Reservation</th>
      <th>Source</th>
      <th>Destination</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_res.each do |car_re| %>
      <tr>
        <td><%= car_re.reservation_id %></td>
        <td><%= car_re.source %></td>
        <td><%= car_re.destination %></td>
        <td><%= link_to 'Show', car_re %></td>
        <td><%= link_to 'Edit', edit_car_re_path(car_re) %></td>
        <td><%= link_to 'Destroy', car_re, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car re', new_car_re_path %>
